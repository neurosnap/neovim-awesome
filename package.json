{
  "name": "neovimcraft",
  "version": "0.0.0",
  "scripts": {
    "dev": "svelte-kit dev --host 0.0.0.0",
    "build:clean": "rm -rf build",
    "build": "svelte-kit build && node scripts/postbuild.js",
    "preview": "svelte-kit preview --host 0.0.0.0",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "husky install",
    "scrape": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/scrape.ts",
    "patch": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/patch.ts",
    "process": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/process.ts",
    "html": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/html.ts",
    "resource": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/resource.ts",
    "resource:clean": "node --no-warnings --es-module-specifier-resolution=node --loader ts-node/esm scripts/resource-clean.ts",
    "upload:clean": "gsutil -m rm -r gs://neovimcraft.com/*",
    "upload": "gsutil -m -h 'Cache-Control:private, max-age=0, no-transform' rsync -r ./build gs://neovimcraft.com",
    "upload:db": "gsutil -m -h 'Cache-Control:private, max-age=0, no-transform' cp ./src/lib/db.json gs://neovimcraft.com/db.json",
    "build:all": "yarn scrape && yarn patch && yarn process && yarn html && yarn build:clean && yarn build",
    "deploy": "yarn build:clean && yarn build && yarn upload:clean && yarn upload && yarn upload:db",
    "deploy:all": "yarn scrape && yarn patch && yarn process && yarn html && yarn deploy"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.139",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.4.10",
    "@types/node-fetch": "^2.5.12",
    "@types/prettier": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": ">=6",
    "just-throttle": "^2.3.1",
    "lint-staged": ">=10",
    "marked": "^2.1.3",
    "node-fetch": "^2.6.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "query-string": "^7.0.1",
    "replace": "^1.2.1",
    "svelte": "3.34.0",
    "svelte-check": "2.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.0.0",
    "typescript": "^4.6.3"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {}
}
